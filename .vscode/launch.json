    "version": "0.2.0",
    "configurations": [
        {
            "name": "RunServiceOnce",
            "type": "debugpy",
            "request": "launch",
            "module": "assemblyline_v4_service.dev.run_service_once",
            "cwd": "${workspaceFolder}",
            "args": [
                "-d",
                "frankenstrings.frankenstrings.FrankenStrings",
                "${file}"
